{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:you-dont-need-lodash-underscore/compatible"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", { "code": 130 }],
    "object-curly-newline": ["error", {
      "minProperties": 6,
      "consistent": true
    }],
    "you-dont-need-lodash-underscore/omit": "off",
    "import/extensions": "off"
  }
}
